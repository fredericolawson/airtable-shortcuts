{
  "manifest_version": 3,
  "name": "Airtable Shortcuts",
  "version": "1.0.12",
  "description": "Custom keyboard shortcuts for Airtable",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.airtable.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["activeTab"],
  "host_permissions": ["*://*.airtable.com/*"],
  "homepage_url": "https://github.com/your-username/airtable-shortcuts",
  "author": "Frederick Lawson"
}
